
public class TestClass {

	/**
	 * @param args
	 */
	public static void main(String[] args) 
	{
		System.out.println(gcdREC(1,0));
	}
	
	
	
	public static long gcdDP(long a, long b)
	{
		return 0;
	}
	
	
	public static long gcdREC(long a, long b)
	{
		if (b == 0) {
			return a;
		} else {
			return gcdREC(b,a%b);
		}
	}

}
